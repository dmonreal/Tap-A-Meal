<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".info"
    tools:layout_editor_absoluteY="25dp">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="80dp"
        android:layout_marginEnd="111dp"
        android:layout_marginTop="16dp"
        android:text="@string/reference_card"
        android:textAllCaps="true"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/tableLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:layout_marginEnd="27dp"
        android:layout_marginStart="27dp"
        android:text="@string/each_block_is_set_to_the_below_calorie_count_tap_each_block_to_learn_more"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/tableLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TableLayout
        android:id="@+id/tableLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="208dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:padding="3dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9">

        <TableRow
            android:layout_width="289dp"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/p8"
                style="@style/Widget.AppCompat.Button.Colored"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="3dp"
                android:background="@android:color/holo_red_dark"
                android:padding="3dp" />

            <TextView
                android:id="@+id/textView33"
                android:layout_width="134dp"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:paddingEnd="10dp"
                android:paddingStart="10dp"
                android:text="@string/_7g_protein" />

            <Button
                android:id="@+id/s9"
                style="@style/Widget.AppCompat.Button.Colored"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="3dp"
                android:background="@android:color/holo_orange_dark"
                android:padding="3dp" />

            <TextView
                android:id="@+id/protein5"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:paddingEnd="10dp"
                android:paddingStart="10dp"
                android:text="@string/_15g_of_carbs" />

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/d"
                style="@style/Widget.AppCompat.Button.Colored"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="3dp"
                android:background="@android:color/holo_blue_dark"
                android:padding="3dp" />

            <TextView
                android:id="@+id/protein6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:paddingEnd="10dp"
                android:paddingStart="10dp"
                android:text="@string/_90_cal_of_dairy" />

            <Button
                android:id="@+id/v"
                style="@style/Widget.AppCompat.Button.Colored"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="3dp"
                android:background="@android:color/holo_green_dark"
                android:padding="3dp" />

            <TextView
                android:id="@+id/protein7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:paddingEnd="10dp"
                android:paddingStart="10dp"
                android:text="@string/tap_for_info" />
        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/f"
                style="@style/Widget.AppCompat.Button.Colored"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="3dp"
                android:background="@android:color/holo_purple"
                android:padding="3dp" />

            <TextView
                android:id="@+id/protein8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:paddingEnd="10dp"
                android:paddingStart="10dp"
                android:text="@string/tap_for_info" />

            <Button
                android:id="@+id/fat"
                style="@style/Widget.AppCompat.Button.Colored"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="3dp"
                android:background="@android:color/holo_orange_light"
                android:padding="3dp" />

            <TextView
                android:id="@+id/protein9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:paddingEnd="10dp"
                android:paddingStart="10dp"
                android:text="@string/_5g_of_fat" />

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="70dp">

            <Button
                android:id="@+id/t"
                style="@style/Widget.AppCompat.Button.Colored"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="3dp"
                android:background="@android:color/black"
                android:padding="3dp" />

            <TextView
                android:id="@+id/protein10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:paddingEnd="10dp"
                android:paddingStart="10dp"
                android:text="@string/_30_free_calories" />

        </TableRow>

    </TableLayout>

    <TableLayout
        android:id="@+id/tableLayout3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="47dp"
        android:layout_marginEnd="15dp"
        android:padding="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout2">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="6dp"
                android:contentDescription="@string/tap_icon"
                app:srcCompat="@drawable/ic_touch_app_black_24dp"
                tools:layout_editor_absoluteX="162dp"
                tools:layout_editor_absoluteY="46dp" />

            <TextView
                android:id="@+id/textView38"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:text="@string/to_record_the_item_simply_tap_it" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="6dp"
                android:contentDescription="@string/home_android_string_fingerprint_icon_content_description"
                app:srcCompat="@drawable/ic_home_black_24dp" />

            <TextView
                android:id="@+id/textView35"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:text="@string/tap_this_to_go_to_the_home_screen" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="6dp"
                android:background="@color/colorPrimaryDark"
                android:contentDescription="@string/info_string_info_button"
                app:srcCompat="@android:drawable/ic_dialog_info" />

            <TextView
                android:id="@+id/textView36"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:text="@string/this_beauty_will_bring_you_back_here" />
        </TableRow>

        <TableRow
            android:layout_width="294dp"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="6dp"
                android:background="@color/colorPrimaryDark"
                android:contentDescription="@string/reset_button"
                app:srcCompat="@android:drawable/ic_menu_revert" />

            <TextView
                android:id="@+id/textView37"
                android:layout_width="276dp"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:text="@string/finished_your_tracking_tap_this_button_to_reset" />
        </TableRow>

    </TableLayout>


    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="0dp"
        android:layout_height="53dp"
        android:layout_marginEnd="47dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/home_button"
        android:onClick="home"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_home_black_24dp" />


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginBottom="1dp"
        android:layout_marginEnd="78dp"
        android:layout_marginStart="60dp"
        android:constraintSet="cwklogo"
        android:contentDescription="@string/cwk_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageButton2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout2"
        app:srcCompat="@drawable/cwklogo" />

</android.support.constraint.ConstraintLayout>